<?php
$arr=array("77","5","5","22","13","55","97","4","796","1","0","9");
$len = count($arr); 
    for ($i = 0; $i < $len - 1; $i++) { 
      for ($j = 0; $j < $len - 1; $j++) { 
          if ($arr[$j] > $arr[$j + 1]) { 
            $tmp = $arr[$j + 1]; 
            $arr[$j + 1] = $arr[$j]; 
            $arr[$j] = $tmp; 
          } 
      } 
    } 
print_r($arr);
